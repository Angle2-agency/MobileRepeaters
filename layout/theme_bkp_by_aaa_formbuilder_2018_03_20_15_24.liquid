<!doctype html>
<!--[if lt IE 7]><html class="no-js lt-ie9 lt-ie8 lt-ie7" lang="en"> <![endif]-->
<!--[if IE 7]><html class="no-js lt-ie9 lt-ie8" lang="en"> <![endif]-->
<!--[if IE 8]><html class="no-js lt-ie9" lang="en"> <![endif]-->
<!--[if IE 9 ]><html class="ie9 no-js"> <![endif]-->
<!--[if (gt IE 9)|!(IE)]><!--> <html class="no-js"> <!--<![endif]-->  
<head>
  <meta charset="utf-8" />
  <!--[if IE]><meta http-equiv='X-UA-Compatible' content='IE=edge,chrome=1' /><![endif]-->

  <title>
    {{ page_title }}{% if current_tags %} &ndash; tagged "{{ current_tags | join: ', ' }}"{% endif %}{% if current_page != 1 %} &ndash; Page {{ current_page }}{% endif %}{% unless page_title contains shop.name %} &ndash; {{ shop.name }}{% endunless %}
  </title>

  {% if page_description %}
    <meta name="description" content="{{ page_description | escape }}" />
  {% endif %}

  <link rel="canonical" href="{{ canonical_url }}" />

  <meta name="viewport" content="width=device-width, initial-scale=1" />
  
  

  {{ content_for_header }}
  

  {{ '//fonts.googleapis.com/css?family=Open+Sans:300,400,600' | stylesheet_tag }}  

  {{ 'normalize.css' | asset_url | stylesheet_tag }}
  {{ 'slick.css' | asset_url | stylesheet_tag }}
  {{ 'jquery.scrollbar.css' | asset_url | stylesheet_tag }}
  {{ 'main.css' | asset_url | stylesheet_tag }}
  {{ 'mq.css' | asset_url | stylesheet_tag }}
  
  {{ 'jquery-3.2.1.min.js'  | asset_url | script_tag }}
  {{ 'slick.min.js'  | asset_url | script_tag }}
  {{ 'jquery.scrollbar.min.js'  | asset_url | script_tag }}
  {{ 'main.js'  | asset_url | script_tag }}
  {{ 'shopstorm-apps.scss.css' | asset_url | stylesheet_tag }}
  {% include 'shopstorm-scripttags' %}
</head>
<body id="{{ page_title | handle }}" class="{% if customer %}customer-logged-in {% endif %}template-{{ template | replace: '.', ' ' | truncatewords: 1, '' | handle }}" >  
  <header id="header">    
      <div class="wrapper clearfix">
        <div class="header__mobile_menu-but">
          <span></span>
        </div>
        <div class="header__mobile_menu">
          <i class="close"></i>
          <ul class="header__mobile_menu-list">
            {% for link in linklists.footer.links %}
            	<li>
            		{% if link.active %}            		
                      <a class="current" href="{{link.url}}">{{ link.title }}</a>                        
            		{% else %}            		
                      <a href="{{link.url}}">{{ link.title }}</a>                        
            		{% endif %}              
            	</li>
            {% endfor %}
          </ul>
        </div>
          <div class="header__logo">
              <a href="/">
                {{'header-logo.png' | asset_url | img_tag}}                  
              </a>
          </div>
          <section class="header__nav clearfix">
            <ul class="header__nav_list clearfix">
              {% for link in linklists.main-menu.links %}
                  {% if link.levels > 0 %}
                      <li class="sub">
                          <span>{{ link.title }}</span>
                          <ul class="sub__list">
                              {% for sub_link in link.links %}
                              <li>
                                  <a href="{{sub_link.url}}">{{sub_link.title}}</a>
                              </li>                    
                              {% endfor %}
                          </ul>
                      </li>        
                  {% else %}
                      <li>
                          <a href="{{link.url}}">{{ link.title }}</a>    
                      </li>            
                  {% endif %}        
              {% endfor %}    
          	</ul>
            <div class="header__nav_params clearfix">
              {% section 'top-nav' %}              
              {% include 'cart' %}  
            </div>
            
          </section>
      </div>
  </header>    
  
  {{content_for_layout}}
  
  <footer id="footer">
    <div class="wrapper clearfix">
      <div class="footer__copyright">Â© Mobile Signal Solutions</div>
      <div class="footer__ask">
        <div class="footer__ask-text"><a href="/blogs/news">Blogs</a></div>
        <div class="footer__ask-text">Have a question?</div>
        <a href="" class="footer__ask-button">ask us</a>
      </div>
    </div>
  </footer>
